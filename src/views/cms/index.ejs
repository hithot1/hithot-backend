    <%- include("scripts") -%>
    <script>
        let totaltile = 30;
        let totalbathware = 45;
        let newLaunch = 43;

        (function($) {  
            "use strict"


            var data = {
                labels: ['Tiles ('+totaltile+')', 'Bathware ('+totalbathware+')', 'New Launch ('+newLaunch+')'],
                series: [{
                            value: totaltile,
                            className: "bg-youtube"
                        },
                        {
                            value: +totalbathware+10,
                            className: "bg-twitter"
                        },
                        {
                            value: +newLaunch+10,
                            className: "bg-facebook"
                        }
                    ]
                    //        colors: ["#333", "#222", "#111"]
            };

            var options = {
                labelInterpolationFnc: function(value) {
                    return value[0]
                }
            };

            var responsiveOptions = [
                ['screen and (min-width: 640px)', {
                    chartPadding: 30,
                    labelOffset: 100,
                    labelDirection: 'explode',
                    labelInterpolationFnc: function(value) {
                        return value;
                    }
                }],
                ['screen and (min-width: 1024px)', {
                    labelOffset: 80,
                    chartPadding: 20
                }]
            ];

            new Chartist.Pie('.ct-pie-chart', data, options, responsiveOptions);


            /*----------------------------------*/

        })(jQuery);
    </script>

