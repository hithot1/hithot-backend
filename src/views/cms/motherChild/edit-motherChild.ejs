        let delete_delivery_rows = [];
        let delete_room_rows = [];
        let delete_why_choose_rows = [];
        let delete_wellness_rows = [];

        $(document).on('click', '.remove_delivery_row', function() {
            delete_delivery_rows.push($(this).data('id'));
            $(this).parents('.previous-delivery-row').remove();
        });

        $(document).on('click', '.remove_room_row', function() {
            delete_room_rows.push($(this).data('id'));
            $(this).parents('.previous-room-row').remove();
        });

        $(document).on('click', '.remove_wellness_row', function() {
            delete_wellness_rows.push($(this).data('id'));
            $(this).parents('.previous-wellness-row').remove();
        });

