                                                            <button class="btn btn-primary ml--40 remove_room_row" data-id="<%= room._id%>" type="button"><i class="fa fa-trash"></i></button>
                                                        </div>
                                                    </div>
                                                    <% } %>

                                                    <div class="row">
                                                        <div class="col-8">
