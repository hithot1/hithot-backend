                                                        </div>

                                                        <div class="col-lg-4">
                                                            <label class="col-lg-12 col-form-label">Wellness Description
                                                                <span class="text-danger">*</span>
                                                            </label>
                                                            <div class="col-lg-12">
                                                                <%=wellness.wellnessDescription%>
                                                            </div>
                                                        </div>
                                                        
                                                        <div class="col-lg-2 mt--35">
                                                            <button class="btn btn-primary ml--40 remove_wellness_row" data-id="<%= wellness._id%>" type="button"><i class="fa fa-trash"></i></button>
                                                        </div>
                                                    </div>
                                                    <% } %>

                                                    <div class="row">
                                                        <div class="col-8">
                                                        </div>
                                                        <div class="col-4 add-btns">
                                                            <button type="button" class="btn btn-primary add-row-wellness"> Add Wellness Details </button>
                                                        </div>
                                                    </div>
                                                </div>
                                                
                                                <div class="customize_section-whychoose">
                                                    <% for (let whyChoose of data.whyChooseGroupData) { %>
