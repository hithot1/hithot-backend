        });
        
        $(document).ready(function() {
            $('.select-tag-hospitalnames').val(<%- JSON.stringify(data.hospitalNames)%>).trigger('change');
            $('.select-tag-doctornames').val(<%- JSON.stringify(data.doctorNames)%>).trigger('change');
            $('.select-tag-patientStories').val(<%- JSON.stringify(data.patientStories)%>).trigger('change');

        });
        $(document).on('click', '#submit-form', async function() {
            const is_form_valid = $("#cms_form").valid();

            if (is_form_valid) {
                let formData = new FormData(document.querySelector('#cms_form'));
                formData.append('delete_delivery_rows', delete_delivery_rows);
                formData.append('delete_room_rows', delete_room_rows);
                formData.append('delete_why_choose_rows', delete_why_choose_rows);
                formData.append('delete_wellness_rows', delete_wellness_rows);

                const resp = await asyncPost('/cms/mother-child/edit','' ,formData);          
                
                if (resp.success) {
                    alert('Mother & Child updated successfully');
                    location.reload();
                } else {
                    if (resp.message) {
                        alert(resp.message);
                    } else {
                        alert(resp.error);
                    }
                }
            }

            
        });

        
