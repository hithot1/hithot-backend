        $(document).ready(function() {
            // $('.select-tag-disease').val(<%- JSON.stringify(data.diseases)%>).trigger('change');
            $('.select-tag-hospitalnames').val(<%- JSON.stringify(data.hospitalNames)%>).trigger('change');
            $('.select-tag-faqs').val(<%- JSON.stringify(data.faqs)%>).trigger('change');

            let checkbox = <%=data.checked%> 
            if (checkbox) {
                document.getElementById("enable").checked = true;
            } else {
                document.getElementById("disable").checked = true;
            }

            $('#enable').change(function() {
                if ($(this).is(":checked")) {
                    document.getElementById("disable").checked = false;
                }
            });

            $('#disable').change(function() {
                if ($(this).is(":checked")) {
                    document.getElementById("enable").checked = false;
                }
            });
        });
        $(document).on('click', '#submit-form', async function() {
            const is_form_valid = $("#cms_form").valid();

            if (is_form_valid) {
                let formData = new FormData(document.querySelector('#cms_form'));
                // formData.append('delete_banner_rows', delete_banner_rows);
                // formData.append('delete_misc_rows', delete_misc_rows);
                // formData.append('delete_technology_rows', delete_technology_rows);

                const resp = await asyncPost('/cms/speciality/edit','' ,formData);    
                
                if (resp.success) {
                    alert('Speciality edited successfully');
                    location.reload();
                } else {
                    if (resp.message) {
                        alert(resp.message);
                    } else {
                        alert(resp.error);
                    }
                }
            }

            
        });

        
    </script>
