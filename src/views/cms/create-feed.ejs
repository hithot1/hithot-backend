<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>Somany CMS</title>
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="/static/images/favicon.ico">
    <%-  include('styles') -%>
</head>

<body>

    
    <%- include('preloader') -%>
    
    <div id="main-wrapper">

       <%- include('nav-header') -%>
       
       <%- include('header') -%>
       
       <%- include('sidebar') -%>

       <!--**********************************
            Content body start
        ***********************************-->
        <div class="content-body">
            <div class="container-fluid">
                <div class="row page-titles mx-0">
                    <div class="col-sm-6 p-md-0">
                        <div class="welcome-text">
                            <h4>Create Feed</h4>
                            
                        </div>
                    </div>
                   
                </div>
                <!-- row -->
                <div class="row">
                    <div class="col-xl-6 col-xxl-12">
                        <form>
                        <div class="card">
                            <div class="card-header">
                                <h4 class="card-title">Tag Category</h4>
                            </div>
                            <div class="card-body">
                                <div class="basic-form">
                                   
                                        <div class="form-group">
                                            <input type="text" class="form-control input-default " placeholder="input-default">
                                        </div>
                                        
                                </div>
                            </div>
                        </div>
                    </form>
                        <div class="card">
                            <div class="card-header">
                                <h4 class="card-title">Post Title</h4>
                            </div>
                            <div class="card-body">
                                <div class="basic-form">
                                    <input type="text" class="form-control input-default " placeholder="input-default">
                                </div>
                            </div>

                            <div class="form-group row pl-2 pl-2 m-0">
                                <label class="col-lg-2 col-form-label">Post Image <span
                                        class="text-danger">*</span>
                                </label>
                                <div class="col-lg-4">
                                    <div class="custom-file">
                                        <input name="thumbnail_image_file" type="file" class="custom-file-input">
                                        <label class="custom-file-label">Choose file</label>
                                    </div>
                                </div>

                                <label class="col-lg-2 col-form-label" >Alt Tag<span
                                    class="text-danger">*</span>
                                </label>
                                <div class="col-lg-4">
                                    <input type="text" class="form-control" name="thumbnail_alt" placeholder="Enter thumbnail alt...">
                                </div>

                                <div class="col-lg-12 ">
                                    <label class="col-lg-12 col-form-label">
                                        Description
                                    </label>
                                    <div class="col-lg-12">
                                        <textarea name="image_description" class="summernote form-control-textarea" placeholder="Page Description" rows="5"></textarea>
                                    </div>
                                </div>
                            </div>
                        </div>
                        
                      
                </div>
            </div>
        </div>
        <!--**********************************
            Content body end
        ***********************************-->


        <%- include("footer") -%>
        
    </div>
    <%- include("scripts") -%>

</body>

</html>